{
  "manifest_version": 2,
  "name": "東京ミニシアター TOP 10",
  "version": "2.0.0",
  "description": "東京23区のミニシアター・名画座の今週のおすすめ映画ランキング。毎日自動更新。",
  "browser_specific_settings": {
    "gecko": {
      "id": "tokyo-mini-theater@example.com",
      "strict_min_version": "109.0"
    }
  },
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    },
    "default_title": "東京ミニシアター TOP 10",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://smata44-sudo.github.io/*"
  ]
}
